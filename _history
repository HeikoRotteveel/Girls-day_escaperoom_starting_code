eruit, rechterwiel vooruit (draai naar links)\n    maqueen.motorRun(maqueen.Motors.M2, maqueen.Dir.CCW, snelheid)\n    maqueen.motorRun(maqueen.Motors.M1, maqueen.Dir.CW, snelheid)\n    while (Math.abs(input.compassHeading() - doelhoek) > 5) {\n        // Voorkomt te snelle herhaling\n        basic.pause(50)\n    }\n    maqueen.motorStop(maqueen.Motors.All)\n}\nlet Rechts = false\nlet doelhoek = 0\nlet beginhoek = 0\nlet snelheid = 0\n// Aangepaste snelheid\nsnelheid = 20\n// Langere tijd per herhaling\nlet draaitijd = 20\n// Langere tijd per herhaling\nbasic.forever(function () {\n    if (maqueen.Ultrasonic() < 12) {\n        Rechts = Math.randomBoolean()\n        if (Rechts) {\n            sla_rechtsaf()\n        } else {\n            sla_linksaf()\n        }\n    } else {\n        rij_rechtdoor()\n    }\n})\n","README.md":"","pxt.json":"{\n    \"name\": \"Escape room test\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\",\n        \"maqueen\": \"github:dfrobot/pxt-maqueen#v1.7.15\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\"\n    ],\n    \"preferredEditor\": \"tsprj\"\n}\n"}}],"shares":[],"lastSaveTime":1742646377510}